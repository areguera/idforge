"                shift 1\n"
"                ;;\n"
"\n"
"            -u | --upper )\n"
"                HELLO_FLAGS=\"${HELLO_FLAGS} upper\"\n"
"                shift 1\n"
"                ;;\n"
"\n"
"            -c | --camel )\n"
"                HELLO_FLAGS=\"${HELLO_FLAGS} camel\"\n"
"                shift 1\n"
"                ;;\n"
"\n"
"            -r | --random )\n"
